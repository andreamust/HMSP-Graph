        <rdfs:comment xml:lang="en">Associates a timeline map to its domain timeline.</rdfs:comment>
        <rdfs:label xml:lang="en">domain timeline</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/NET/c4dm/timeline.owl#rangeTimeLine -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/timeline.owl#rangeTimeLine">
        <rdfs:domain rdf:resource="http://purl.org/NET/c4dm/timeline.owl#TimeLineMap"/>
