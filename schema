    <!-- http://purl.org/ontology/mo/produced_score -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/produced_score">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/Arrangement"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/Composition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Score"/>
        <rdfs:comment xml:lang="en">Associates an arrangement or a composition event to a score product (score here does not refer to a published score, but more an abstract arrangement of a particular work).</rdfs:comment>
        <rdfs:label xml:lang="en">produced_score</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/produced_signal -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/produced_signal">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Recording"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:comment xml:lang="en">Associates a Recording to the outputted signal.</rdfs:comment>
        <rdfs:label xml:lang="en">produced signal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/produced_sound -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/produced_sound">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Sound"/>
        <rdfs:comment xml:lang="en">Associates a Performance to a physical Sound that is being produced by it.</rdfs:comment>
        <rdfs:label xml:lang="en">produced sound</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/produced_work -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/produced_work">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Composition"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicalWork"/>
        <rdfs:comment xml:lang="en">Associates a composition event to the produced MusicalWork. For example, this property could link the event corresponding to the composition of the Magic Flute in 1782 to the Magic Flute musical work itself. This musical work can then be used in particular performances.</rdfs:comment>
        <rdfs:label xml:lang="en">produced work</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/published_as -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/published_as">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicalExpression"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
        <rdfs:comment xml:lang="en">Links a musical expression (e.g. a signal or a score) to one of its manifestations (e.g. a track on a particular record or a published score).</rdfs:comment>
        <rdfs:label xml:lang="en">published as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/recorded_as -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/recorded_as">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/mo/records"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:comment xml:lang="en">This is a shortcut property, allowing to bypass all the Sound/Recording steps. This property allows to directly link a Performance to the recorded Signal. This is recommended for &quot;normal&quot; users. However, advanced users wanting to express things such as the location of the microphone will have to create this shortcut as well as the whole workflow, in order to let the &quot;normal&quot; users access simply the, well, simple information:-) .</rdfs:comment>
        <rdfs:label xml:lang="en">recorded as</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/recording_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/recording_of">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Recording"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Sound"/>
        <rdfs:comment xml:lang="en">Associates a Recording event to a physical Sound being recorded. For example, I might use this property to associate a given recording, done using my cell phone, to the sound produced by a particular performance of the magic flute.</rdfs:comment>
        <rdfs:label xml:lang="en">recording of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/records -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/records">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:comment xml:lang="en">This is the inverse of the shortcut property recordedAs, allowing to relate directly a performance to a signal.</rdfs:comment>
        <rdfs:label xml:lang="en">records</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/sampled_version_of -->

